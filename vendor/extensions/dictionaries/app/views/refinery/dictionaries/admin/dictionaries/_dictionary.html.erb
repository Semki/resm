<li class='clearfix record <%= cycle("on", "on-hover") %>' id="<%= dom_id(dictionary) -%>">
  <span class='title'>
    <%= dictionary.classname %>

  </span>
  <span class='actions'>
    
    <%= link_to refinery_icon_tag("application_go.png"), refinery.dictionaries_dictionary_path(dictionary),
        :title => t('.view_live_html'),
        :target => "_blank" %>
    
    <%= link_to refinery_icon_tag("application_edit.png"), refinery.edit_dictionaries_admin_dictionary_path(dictionary),
         :title => t('.edit') %>
    <%= link_to refinery_icon_tag("delete.png"), refinery.dictionaries_admin_dictionary_path(dictionary),
        :class => "cancel confirm-delete",
        :title => t('.delete'),
        :confirm => t('message', :scope => 'refinery.admin.delete', :title => dictionary.classname),
        :method => :delete %>
  </span>
</li>
