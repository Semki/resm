<li class='clearfix record <%= cycle("on", "on-hover") %>' id="<%= dom_id(realestateobject) -%>">
  <span class='title'>
    <%= realestateobject.name %>

  </span>
  <span class='actions'>
    
    <%= link_to refinery_icon_tag("application_go.png"), refinery.realestateobjects_realestateobject_path(realestateobject),
        :title => t('.view_live_html'),
        :target => "_blank" %>
    
    <%= link_to refinery_icon_tag("application_edit.png"), refinery.edit_realestateobjects_admin_realestateobject_path(realestateobject),
         :title => t('.edit') %>
    <%= link_to refinery_icon_tag("delete.png"), refinery.realestateobjects_admin_realestateobject_path(realestateobject),
        :class => "cancel confirm-delete",
        :title => t('.delete'),
        :confirm => t('message', :scope => 'refinery.admin.delete', :title => realestateobject.name),
        :method => :delete %>
  </span>
</li>
