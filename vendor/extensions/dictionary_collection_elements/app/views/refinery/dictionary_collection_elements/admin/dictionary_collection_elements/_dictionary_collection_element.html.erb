<li class='clearfix record <%= cycle("on", "on-hover") %>' id="<%= dom_id(dictionary_collection_element) -%>">
  <span class='title'>
    <%= dictionary_collection_element. %>

  </span>
  <span class='actions'>
    
    <%= link_to refinery_icon_tag("application_go.png"), refinery.dictionary_collection_elements_dictionary_collection_element_path(dictionary_collection_element),
        :title => t('.view_live_html'),
        :target => "_blank" %>
    
    <%= link_to refinery_icon_tag("application_edit.png"), refinery.edit_dictionary_collection_elements_admin_dictionary_collection_element_path(dictionary_collection_element),
         :title => t('.edit') %>
    <%= link_to refinery_icon_tag("delete.png"), refinery.dictionary_collection_elements_admin_dictionary_collection_element_path(dictionary_collection_element),
        :class => "cancel confirm-delete",
        :title => t('.delete'),
        :confirm => t('message', :scope => 'refinery.admin.delete', :title => dictionary_collection_element.),
        :method => :delete %>
  </span>
</li>
