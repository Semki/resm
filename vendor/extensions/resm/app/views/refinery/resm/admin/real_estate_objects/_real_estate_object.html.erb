<li class='clearfix record <%= cycle("on", "on-hover") %>' id="<%= dom_id(real_estate_object) -%>">
  <span class='title'>
    <%= real_estate_object.name %>

  </span>
  <span class='actions'>
    
    <%= link_to refinery_icon_tag("application_go.png"), refinery.resm_real_estate_object_path(real_estate_object),
        :title => t('.view_live_html'),
        :target => "_blank" %>
    
    <%= link_to refinery_icon_tag("application_edit.png"), refinery.edit_resm_admin_real_estate_object_path(real_estate_object),
         :title => t('.edit') %>
    <%= link_to refinery_icon_tag("delete.png"), refinery.resm_admin_real_estate_object_path(real_estate_object),
        :class => "cancel confirm-delete",
        :title => t('.delete'),
        :confirm => t('message', :scope => 'refinery.admin.delete', :title => real_estate_object.name),
        :method => :delete %>
  </span>
</li>
