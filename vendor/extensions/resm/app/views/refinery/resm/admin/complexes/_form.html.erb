<%= form_for [refinery, :resm_admin, @complex] do |f| -%>
  <%= render '/refinery/admin/error_messages',
              :object => @complex,
              :include_object_name => true %>

  <%= render "locale_picker",
              :current_locale => Thread.current[:globalize_locale] if Refinery.i18n_enabled? %>

  
  <div class='field'>
    <%= f.label :group_name -%>
    <%= f.text_field :group_name, :class => 'larger widest'  -%>

  </div>
  
  <div class='field'>
    <%= f.label :group_type -%>
    <%= f.select :group_type, options_from_collection_for_select(self.get_items_by_dictionary(:group_type), :id, :name) -%>

  </div>
  
  <div class='field'>
    <%= f.label :company_id -%>
    <%= f.number_field :company_id  -%>

  </div>
  
  <div class='field'>
    <%= f.label :tip_location -%>
    <%= f.select :tip_location, options_from_collection_for_select(self.get_items_by_dictionary(:tip_location), :id, :name) -%>

  </div>
  
  <div class='field'>
    <%= f.label :price_sale_group -%>
    <%= f.number_field :price_sale_group  -%>

  </div>
  
  <div class='field'>
    <%= f.label :invest_price_group -%>
    <%= f.number_field :invest_price_group  -%>

  </div>
  
  <div class='field'>
    <%= f.label :invest_share_group -%>
    <%= f.number_field :invest_share_group  -%>

  </div>
  
  <div class='field'>
    <%= f.label :geo_country -%>
    <%= f.select :geo_country, options_from_collection_for_select(self.get_items_by_dictionary(:geo_country), :id, :name) -%>

  </div>
  
  <div class='field'>
    <%= f.label :geo_region -%>
    <%= f.select :geo_region, options_from_collection_for_select(self.get_items_by_dictionary(:geo_region), :id, :name) -%>

  </div>
  
  <div class='field'>
    <%= f.label :geo_subregion -%>
    <%= f.select :geo_subregion, options_from_collection_for_select(self.get_items_by_dictionary(:geo_subregion), :id, :name) -%>

  </div>
  
  <div class='field'>
    <%= f.label :geo_cantone -%>
    <%= f.select :geo_cantone, options_from_collection_for_select(self.get_items_by_dictionary(:geo_cantone), :id, :name) -%>

  </div>
  
  <div class='field'>
    <%= f.label :geo_city -%>
    <%= f.select :geo_city, options_from_collection_for_select(self.get_items_by_dictionary(:geo_city), :id, :name) -%>

  </div>
  
  <div class='field'>
    <%= f.label :geo_district -%>
    <%= f.select :geo_district, options_from_collection_for_select(self.get_items_by_dictionary(:geo_district), :id, :name) -%>

  </div>
  
  <div class='field'>
    <%= f.label :building_stage_group -%>
    <%= f.select :building_stage_group, options_from_collection_for_select(self.get_items_by_dictionary(:building_stage_group), :id, :name) -%>

  </div>
  
  <div class='field'>
    <%= f.label :land_density_res_group -%>
    <%= f.number_field :land_density_res_group  -%>

  </div>
  
  <div class='field'>
    <%= f.label :land_density_com_group -%>
    <%= f.number_field :land_density_com_group  -%>

  </div>
  
  <div class='field'>
    <%= f.label :beach_dist -%>
    <%= f.number_field :beach_dist  -%>

  </div>
  
  <div class='field'>
    <%= f.label :availability_group_sale -%>
    <%= f.select :availability_group_sale, options_from_collection_for_select(self.get_items_by_dictionary(:availability_group_sale), :id, :name) -%>

  </div>
  
  <div class='field'>
    <%= f.label :availability_group_rent -%>
    <%= f.select :availability_group_rent, options_from_collection_for_select(self.get_items_by_dictionary(:availability_group_rent), :id, :name) -%>

  </div>
  
  <div class='field'>
    <%= f.label :group_block -%>
    <%= f.select :group_block, options_from_collection_for_select(self.get_items_by_dictionary(:group_block), :id, :name) -%>

  </div>
  
  <div class='field'>
    <%= f.label :features -%>
    <%= f.select :features, options_from_collection_for_select(self.get_items_by_dictionary(:features), :id, :name) -%>

  </div>
  
  <div class='field'>
    <%= f.label :pool_type_group -%>
    <%= f.select :pool_type_group, options_from_collection_for_select(self.get_items_by_dictionary(:pool_type_group), :id, :name) -%>

  </div>
  
  <div class='field'>
    <%= f.label :pool_heated_group -%>
    <%= f.check_box :pool_heated_group, :checked => @complex[:pool_heated_group]  -%>

  </div>
  
  <div class='field'>
    <%= f.label :parking_type_group -%>
    <%= f.select :parking_type_group, options_from_collection_for_select(self.get_items_by_dictionary(:parking_type_group), :id, :name) -%>

  </div>
  
  <div class='field'>
    <%= f.label :parking_levels -%>
    <%= f.number_field :parking_levels  -%>

  </div>
  
  <div class='field'>
    <%= f.label :area_total_group -%>
    <%= f.number_field :area_total_group  -%>

  </div>
  
  <div class='field'>
    <%= f.label :area_basement_group -%>
    <%= f.number_field :area_basement_group  -%>

  </div>
  
  <div class='field'>
    <%= f.label :area_garden -%>
    <%= f.number_field :area_garden  -%>

  </div>
  
  <div class='field'>
    <%= f.label :area_roof -%>
    <%= f.number_field :area_roof  -%>

  </div>
  
  <div class='field'>
    <%= f.label :area_parking_group -%>
    <%= f.number_field :area_parking_group  -%>

  </div>
  
  <div class='field'>
    <%= f.label :built_date_group -%>
    <%= f.number_field :built_date_group  -%>

  </div>
  
  <div class='field'>
    <%= f.label :built_duration_group -%>
    <%= f.number_field :built_duration_group  -%>

  </div>
  
  <div class='field'>
    <%= f.label :built_deliverydate_group -%>
    <%= f.date_select :built_deliverydate_group  -%>

  </div>
  
  <div class='field'>
    <%= f.label :levels_building -%>
    <%= f.number_field :levels_building  -%>

  </div>
  
  <div class='field'>
    <%= f.label :levels_lot -%>
    <%= f.number_field :levels_lot  -%>

  </div>
  
  <div class='field'>
    <%= f.label :gps_lat_group -%>
    <%= f.number_field :gps_lat_group  -%>

  </div>
  
  <div class='field'>
    <%= f.label :gps_lon_group -%>
    <%= f.number_field :gps_lon_group  -%>

  </div>
  
  <%= render '/refinery/admin/form_actions', :f => f,
             :continue_editing => false,
             :delete_title => t('delete', :scope => 'refinery.complexes.admin.complexes.complex'),
             :delete_confirmation => t('message', :scope => 'refinery.admin.delete', :title => @complex.group_name) %>
<% end -%>
