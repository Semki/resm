    <%= f.number_field :price_rent_long  -%>

  </div>
  
  <div class='field'>
    <%= f.label :invest_price -%>
    <%= f.number_field :invest_price  -%>

  </div>
  
  <div class='field'>
    <%= f.label :invest_share -%>
    <%= f.number_field :invest_share  -%>

  </div>
  
  <div class='field'>
