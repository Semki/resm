  </div>
  
  <div class='field'>
    <%= f.label :price_sale -%>
    <%= f.number_field :price_sale  -%>

  </div>
  
  <div class='field'>
    <%= f.label :price_rent_short -%>
    <%= f.number_field :price_rent_short  -%>

  </div>
  
  <div class='field'>
