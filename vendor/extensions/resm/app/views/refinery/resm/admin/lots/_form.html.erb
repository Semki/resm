  <%= render '/refinery/admin/form_actions', :f => f,
             :continue_editing => false,
             :delete_title => t('delete', :scope => 'refinery.lots.admin.lots.lot'),
             :delete_confirmation => t('message', :scope => 'refinery.admin.delete', :title => @lot.lot_code) %>
