<%= form_for [refinery, :resm_admin, @lot] do |f| -%>
  <%= render '/refinery/admin/error_messages',
              :object => @lot,
              :include_object_name => true %>

  <%= render "locale_picker",
              :current_locale => Thread.current[:globalize_locale] if Refinery.i18n_enabled? %>

  
  <div class='field'>
    <%= f.label :lot_code -%>
    <%= f.text_field :lot_code, :class => 'larger widest'  -%>

  </div>
  
  <div class='field'>
    <%= f.label :group_id -%>
    <%= f.number_field :group_id  -%>

  </div>
  
  <div class='field'>
    <%= f.label :company_id -%>
    <%= f.number_field :company_id  -%>

  </div>
  
  <div class='field'>
    <%= f.label :bathscount -%>
    <%= f.number_field :bathscount  -%>

  </div>
  
  <div class='field'>
    <%= f.label :bedscount -%>
    <%= f.number_field :bedscount  -%>

  </div>
  
  <div class='field'>
    <%= f.label :lot_type -%>
    <%= f.select :lot_type, options_from_collection_for_select(self.get_items_by_dictionary(:lot_type), :id, :name) -%>

  </div>
  
  <div class='field'>
    <%= f.label :category_type -%>
    <%= f.select :category_type, options_from_collection_for_select(self.get_items_by_dictionary(:category_type), :id, :name) -%>

  </div>
  
  <div class='field'>
